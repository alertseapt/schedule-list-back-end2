# Configurações do Servidor API Schedule Management
PORT=3000
NODE_ENV=development

# Configurações do Banco de Dados MySQL/MariaDB
# Substitua pelos dados do seu servidor de banco de dados
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=sua_senha_aqui

# Configurações de Segurança JWT
# IMPORTANTE: Gere uma chave secreta forte para produção
JWT_SECRET=sua_chave_secreta_jwt_super_segura_aqui
JWT_EXPIRE=7d

# Configurações de Rate Limiting
# 100 requisições por 15 minutos por IP
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# URL do Frontend (opcional)
# Configure a URL do seu frontend para CORS
FRONTEND_URL=http://localhost:3000

# Configurações de Log (opcional)
LOG_LEVEL=info

